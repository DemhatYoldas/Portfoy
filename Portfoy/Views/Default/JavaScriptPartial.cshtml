<!-- CV buttton script kodu-->
<script>
        document.getElementById('downloadButton').addEventListener('click', function () {
            // CV dosyanızın URL'sini buraya yazın
            var cvUrl = '/Tema/Cv/DemhatYoldaşCv.pdf';

            // Yeni bir a etiketi oluşturun ve CV dosyasının indirme bağlantısını ayarlayın
            var downloadLink = document.createElement('a');
            downloadLink.href = cvUrl;
            downloadLink.download = 'DemhatYoldaşCv.pdf'; // İndirilen dosyanın adı

            // Oluşturulan bağlantıyı tıklayarak indirme işlemi gerçekleştirin
            downloadLink.click();
        });
</script>
