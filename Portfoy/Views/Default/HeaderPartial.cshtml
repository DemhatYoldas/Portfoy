@model List<Portfoy.Models.TblProfile>

@foreach (var item in Model)
{
    <div class="container-fluid bg-light my-6 mt-0" id="home">
        <div class="container">

            <div class="row g-5 align-items-center">
                <div class="col-lg-6 py-6 pb-0 pt-lg-0">
                    <h3 class="text-primary mb-3">@item.Header</h3>
                    <h1 class="display-3 mb-3">@item.NameSurname</h1>
                    <h2 class="typed-text-output d-inline"></h2>
                    <div class="typed-text d-none">@item.Title</div>
                    <div class="d-flex align-items-center pt-5">
                        <a id="downloadButton" class="btn btn-primary py-3 px-4 me-5">CV İndir</a>
                        <a href="@item.GtihupUrl" class="btn btn-success py-3 px-4 me-5">Githup</a>
                    </div>
                </div>
                <div class="col-lg-6">
                    <img id="myImage" class="img-fluid" src="~/Tema/img/DemhatYoldas.png" alt="">
                </div>
            </div>

        </div>
    </div>
}

